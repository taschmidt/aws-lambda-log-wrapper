{
  "name": "aws-lambda-log-wrapper",
  "version": "1.1.7",
  "description": "A tiny package to wrap the awesome LambdaLog library for use with AWS SDK JS v3",
  "main": "./dist/index",
  "types": "./dist/index",
  "repository": "https://github.com/taschmidt/aws-lambda-log-wrapper",
  "author": "Tim Schmidt <tim@schmidthole.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "build": "rm -rf dist/ && tsc",
    "build:release": "npm-run-all lint test build",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "yarn run build",
    "test": "jest"
  },
  "lint-staged": {
    "src/**/*.ts": "eslint --fix"
  },
  "dependencies": {
    "@aws-sdk/types": "^3.18.0",
    "json-truncate": "3.0.0",
    "lambda-log": "^3.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-sts": "^3.18.0",
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "babel-jest": "^27.0.2",
    "eslint": "^7.28.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
